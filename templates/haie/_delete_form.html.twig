<form method="post" action="{{ path('haie_delete', {'code': haie.code}) }}" onsubmit="return confirm('Êtes vous sûr de vouloir supprimer cette haie ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ haie.code) }}">
    <button class="btn btn-danger btn-lg">Supprimer</button>
</form>
